import java.util.Scanner;
public class A5T12{
  public static void main(String[]args){
    Scanner sc=new Scanner(System.in);
    int array[]=new int[5];
    int max;
    for(int i=0; i<array.length; i++){
      System.out.println("Please, enter a number");
      array[i]=sc.nextInt();
      if(i==0)
        max=array[i];
      else{
        if(array[i]>max)
          max=array[i];
      }
    }
    System.out.println(max);
  }
}