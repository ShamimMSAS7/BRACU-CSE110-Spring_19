import java.util.Scanner;
public class A4T24{
  public static void main(String[]args){
    Scanner sc=new Scanner(System.in);
    System.out.println("Please, enter the number");
    int num=sc.nextInt();
    for(int i=1; i<num; i++){
      System.out.print(i+", ");
    }
    System.out.print(num);
    sc.close();
  }
}