import java.util.Scanner;
public class L6P5{
  public static void main(String[]args){
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter the number of row");
    int row=sc.nextInt();
    
    int c=1;
    while(c<=row){
      int i=1;
      while(i<c){
      System.out.print("*");
      i++;
      }
    System.out.println("*");
    c++;
    }
    sc.close();
  }
  
}  
