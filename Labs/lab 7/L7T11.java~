
public class L7T11{
  public static void main(String[]args){
    
    System.out.println("All prime numbers between 40 and 50 are");
    
    for(int num=40; num<=50; num++){
      for(int i=1; i<=num; i++){
        if(num%i==0)
          count++;
      }
      if(count==2)
        System.out.print(num+",");
    }
  }
}
