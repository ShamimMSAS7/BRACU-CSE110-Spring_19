import java.util.Scanner;
public class L7T5{
  public static void main(String[]args){
    Scanner sc=new Scanner(System.in);
    for(i=1; i<=20; i++){
      System.out.println("Please, enter a number");
      int num=sc.nextInt();
      if(num%2==0)
        System.out.println("Even number");
      else
        System.out.println("Odd number");
    }
    
  }
}